    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.15.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "dotenv": "^17.2.3",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.9.2"
