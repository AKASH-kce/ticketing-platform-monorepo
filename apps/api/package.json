    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@repo/database": "workspace:*",
    "@repo/pricing-engine": "workspace:*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "drizzle-orm": "^0.36.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
